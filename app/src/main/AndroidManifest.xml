<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.masai.sainath.writenotesapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/notesicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/notesicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.WriteNotesApp">
        <activity android:name=".Activity.UpdateNoteActivity"/>
        <activity android:name=".Activity.InsertNotesActivity" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.SpalshScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>